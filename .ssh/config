Host *
	AddKeysToAgent yes
	UseKeychain yes
	IdentityFile ~/.ssh/id_rsa

Host lugh
	User ajm
	HostName lugh.ss.cx
	ForwardAgent no

Host thor
	User ajm
	HostName thor.ss.cx
	ForwardAgent yes

Host whatbox
	User ajm
	HostName venus.whatbox.ca
	ForwardAgent yes

Host sambox
	User ajm
	HostName 192.168.1.12
	ForwardAgent no
	ProxyJump lugh

Host jon
	User monks
	HostName kate.money
	ForwardAgent yes

Host backup
	User root
	HostName 57269.zfs.rsync.net

Host devbox
	# DEVBOX_HOST
	Hostname 13.52.80.163
	ProxyJump sambox
	CheckHostIP no
	User ubuntu
	ForwardAgent yes
	ExitOnForwardFailure yes
	ServerAliveInterval 30
	ServerAliveCountMax 5
	StrictHostKeyChecking no
	StreamLocalBindUnlink yes

