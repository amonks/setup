	User ajm
	HostName rhea.whatbox.ca
	ForwardAgent yes

Host sambox
	User andrewmonks
	HostName 192.168.0.190

Host jon
	User monks
	HostName kate.money
	ForwardAgent yes

Host nencatacoa
	User ajm
	HostName 192.168.0.141

