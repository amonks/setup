	User root
	HostName 57269.zfs.rsync.net

Host proxied-devbox
	ProxyJump sambox
	CheckHostIP no
	User ubuntu
	ForwardAgent yes
	ExitOnForwardFailure yes
	ServerAliveInterval 30
	ServerAliveCountMax 5
	StrictHostKeyChecking no
	StreamLocalBindUnlink yes

